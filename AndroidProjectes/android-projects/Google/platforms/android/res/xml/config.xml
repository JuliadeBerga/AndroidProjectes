<?xml version='1.0' encoding='utf-8'?>
<widget id="com.natescode.compass" version="1.0.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <name>Compass</name>
    <description>Basic Compass app</description>
    <author email="nate@natescode.com" href="http://natescode.com">Nathan Hedglin</author>
    <preference name="loglevel" value="DEBUG" />
    <preference name="phonegap-version" value="3.3.0" />
</widget>
